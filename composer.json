{
  "name": "phpchassis/youvids",
  "description": "PhpChassis, a Simple PHP Framework built with Layered Architecture in mind!",
  "type": "project",
  "authors": [
    {
      "name": "Tharwat Sakeldien",
      "email": "me@tharwatsw.com",
      "role": "Software Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "youvids\\": "src",
      "testcase\\": "tests",
      "phpchassis\\": "vendor/phpchassis/ddd/src"
    }
  },
  "require": {
    "php": ">= 7.3.*",
    "psr/log": "1.1.*",
    "psr/http-message": "1.0.*",
    "psr/container": "1.0.*",
    "psr/simple-cache": "1.0.*",
    "beberlei/assert": "3.2.*",
    "mailgun/mailgun-php": "1.7.*",
    "maximebf/debugbar": "1.15.*",
    "envms/fluentpdo": "^2.1.0",
    "zendframework/zend-permissions-acl": "2.7.*",
    "zendframework/zend-permissions-rbac": "3.0.*",
    "eftec/bladeone": "3.30"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 1800
  }
}